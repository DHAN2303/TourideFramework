    <script>
        $(function () {
            $("body").on('click', 'a, button', function () {
                ResetThisSession();
            });
        });

        const timeInSecondsAfterSessionOut = 600;
        let secondTick = 0;

        function ResetThisSession() {
            secondTick = 0;
        }

        function StartThisSessionTimer() {
            secondTick++;
            let timeLeft = ((timeInSecondsAfterSessionOut - secondTick) / 60).toFixed(0);

            timeLeft = timeInSecondsAfterSessionOut - secondTick;

            $("#spanTimeLeft").html(timeLeft);
            if (secondTick > timeInSecondsAfterSessionOut) {
                clearTimeout(tick);
                console.log(timeLeft);
                window.location = "https://logindev.otokoc.com.tr/Account/Logout/";
                return;
            }
            tick = setTimeout("StartThisSessionTimer()", 1000);
        }

        StartThisSessionTimer();
    </script>

